<Window x:Class="Geoban.Sharing.WPFClient.Views.VehicleView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:vm="clr-namespace:Geoban.Sharing.WPFClient.ViewModels"
        xmlns:local="clr-namespace:Geoban.Sharing.WPFClient.Views"
        mc:Ignorable="d"
        Title="VehicleView" Height="300" Width="300">

    <Window.DataContext>
        <vm:VehicleViewModel />
    </Window.DataContext>
    
    <Grid>

        <TextBlock Text="{Binding Vehicle.Name}" 
                   HorizontalAlignment="Center"
                   VerticalAlignment="Center" />

    </Grid>
</Window>
